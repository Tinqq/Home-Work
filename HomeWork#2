
Console.WriteLine("Input your Age: ");
byte a = byte.Parse(Console.ReadLine());
Console.WriteLine("Your Age is: " + a);

Console.WriteLine("How many you want to deposit into our company (from -1 000 000 000 000 to 1 000 000 000 000) : ");
long b = long.Parse(Console.ReadLine());
Console.WriteLine("Value of your deposite is : " + b);

Console.WriteLine("Input any letter to create security password of your account (any one symbol): ");

try
{

    char c = char.Parse(Console.ReadLine());
    Console.WriteLine("You password will be: " + c);
}
catch (Exception)
{
    Console.WriteLine("Incorrect Symbol");
}

Console.WriteLine("\tHello to out World! Welcome!");


